    .featured h1 {
      font-size: 30px;
    }
  
     .wrapper {
      grid-template-areas:                          /* Ændring af grid-layoutet, ændres til dette ved mobil-br.poitnt*/
        "featuredMain featuredMain featuredMain"    /* Det er denne orden man ønsker at få vist kolonnerne i. Der kan byttes om på rækkefølgen*/
        "contentMain contentMain contentMain"
        "featuredSub featuredSub featuredSub" 
        "contentSub contentSub contentSub";
        color: #FFF;
    }
    
  .mobile-hide {                /* "hide" og "show" skal bruges i javascript for at vise eller skjule*/
      display: none;
    }
  
  .mobile-show{                 /* "hide" og "show" skal bruges i javascript for at vise eller skjule*/
      display:block;
  }
  
    .burger-icon {
      display: inline-block;
    }
  
    .footer > div{          /* Dette vælger alle "childs" af footer propertien og de kaldes alle "div" i dette tilfælde */
      grid-column: 1/5;
    }
  
    .sub-featured {
       grid-template-columns: 1fr;
    }
  }  

